<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cuit.system.log.mapper.LogMapper">

    <delete id="removeAll">
        #正序排序后删除表中前50条记录
        DELETE  FROM log  order by id asc limit 50;
    </delete>
</mapper>
